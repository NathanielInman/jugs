{
  "name": "<%= name %>",
  "version": "<%= appVersion %>",
  "description": "<%= appDescription %>",
  "main": "gulpfile.babel.js",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= userName %>/<%= name %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= userName %>/<%= name %>/issues"
  },
  "scripts": {
    "build:clean": "rm -rf ./dist && mkdir ./dist",
    "build:assets": "cp -rf src/assets/* ./dist",
    "build": "npm run build:clean && npm run build:assets",
    "prestart": "npm run build",
    "start": "node ./node_modules/webpack/bin/webpack.js --config ./config.babel.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "browser-sync": "^2.18.2",
    "browser-sync-webpack-plugin": "^1.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-loader": "^1.6.1",
    "estraverse-fb": "^1.3.1",
    "file-loader": "^0.9.0",
    "jade-static-loader": "^0.3.0",
    "poststylus": "^0.2.2",
    "source-loader": "^0.2.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.16.2"
  },
  "readmeFilename": "README.md",
  "homepage": "https://github.com/<%= userName %>/<%= name %>"
}
