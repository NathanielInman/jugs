{
  "name": "<%= name %>",
  "version": "<%= appVersion %>",
  "description": "<%= appDescription %>",
  "main": "config.babel.js",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= userName %>/<%= name %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= userName %>/<%= name %>/issues"
  },
  "scripts": {
    "build:clean": "rm -rf ./dist && mkdir ./dist",
    "build:assets": "cp -rf src/assets/* ./dist",
    "build": "npm run build:clean && npm run build:assets",
    "prestart": "npm run build",
    "start": "node ./node_modules/webpack/bin/webpack.js --config ./config.babel.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "estraverse-fb": "^1.3.1",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template-pug": "^1.1.1",
    "poststylus": "^0.2.3",
    "source-loader": "^0.2.0",
    "source-map-loader": "^0.2.1",
    "pug": "^2.0.0-beta.12",
    "pug-loader": "^2.3.0",
    "style-loader": "^0.16.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "ion-cloud": "^1.1.0"
  },
  "readmeFilename": "README.md",
  "homepage": "https://github.com/<%= userName %>/<%= name %>"
}
